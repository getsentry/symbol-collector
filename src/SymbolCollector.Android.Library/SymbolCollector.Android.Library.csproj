<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net7.0-android</TargetFramework>
    <!-- To get access to Microsoft.Maui.Media (screenshots) isn't available via NuGet-->
    <UseMauiEssentials>true</UseMauiEssentials>
    <UseSentryCLI>false</UseSentryCLI>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="../SymbolCollector.Core/SymbolCollector.Core.csproj" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Logging" Version="7.0.0" />
    <PackageReference Include="Sentry" Version="3.35.1" />
    <PackageReference Include="Sentry.Extensions.Logging" Version="3.35.1" />
    <PackageReference Include="System.Net.Http" Version="4.3.4" />
  </ItemGroup>

</Project>
