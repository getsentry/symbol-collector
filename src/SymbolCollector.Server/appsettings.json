{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "GoogleCloud": {
    "JsonCredentialParameters": {
      "type": "service_account",
      "ProjectId": null,
      "PrivateKey": null,
      "ClientEmail": null,
      "ClientId": null
    }
  },
  "Sentry": {
    "AttachStackTrace": true,
    "Debug": true,
    "DiagnosticsLevel": "Info"
  },
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://localhost:5000"
      }
    }
  }
}
