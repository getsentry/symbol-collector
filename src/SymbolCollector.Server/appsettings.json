{
  "FileSizeLimitBytes": 2000000000,
  "AllowedHosts": "*",
  "GoogleCloud": {
    "JsonCredentialParameters": {
      "type": "service_account",
      "ProjectId": null,
      "PrivateKey": null,
      "ClientEmail": null,
      "ClientId": null
    }
  },
  "Sentry": {
    "AttachStackTrace": true,
    "Debug": true,
    "DiagnosticsLevel": "Info",
    "Dsn": null,
    "DefaultTags": {
      "app": "SymbolCollector.Server"
    }
  },
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://localhost:5000"
      }
    },
    "Limits": {
      "MaxRequestBodySize": 2000000000
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "Sentry": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Sentry",
        "Args": {
          "MinimumBreadcrumbLevel": "Debug",
          "MinimumEventLevel": "Warning"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName" ]
  }
}
